{
  "name": "god-state",
  "version": "0.1.0",
  "description": "Convenient state management for React",
  "license": "MIT",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/es/index.js",
  "author": "Micwaits Microcood <microcood@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/microcood/god-state"
  },
  "bugs": {
    "url": "https://github.com/microcood/god-state/issues"
  },
  "scripts": {
    "build:es": "cross-env babel src --out-dir dist/es --ignore __tests__",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir dist/commonjs --ignore __tests__",
    "build:umd": "cross-env NODE_ENV=production webpack",
    "build": "npm run build:es && npm run build:commonjs && npm run build:umd",
    "lint": "eslint src",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "reactjs",
    "state",
    "context",
    "decorator"
  ],
  "babel": {
    "presets": [
      "es2015-no-commonjs",
      "stage-2"
    ],
    "plugins": [
      "lodash"
    ],
    "env": {
      "commonjs": {
        "plugins": [
          "lodash",
          "transform-es2015-modules-commonjs"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {},
    "env": {
      "jest": true
    }
  },
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015-no-commonjs": "^0.0.2",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.5",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^22.4.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-testing-library": "^3.1.0",
    "webpack": "^4.8.2",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {
    "lodash": "^4.17.10"
  }
}
